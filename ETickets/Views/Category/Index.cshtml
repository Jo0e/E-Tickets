@model ICollection<Category>

<h2>Categories</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                @item.Name
            </td>

            <td>
                    <a href="/Category/Details?id=@item.Id">All Movies</a>
            </td>
        </tr>
        }
    </tbody>
</table>